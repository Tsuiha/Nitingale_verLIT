{
    "keyboard_name": "nitingale_lit",
    "manufacturer": "Tsuiha",
    "maintainer": "Tsuiha",
	"processor": "RP2040",
    "bootloader": "rp2040",
	"board": "GENERIC_RP_RP2040",
    "features": {
        "mousekey": true,
    },
	"build": {
                "debounce_type":"sym_eager_pk",
                "firmware_format": "uf2"
    },
	"debounce":5,

    "matrix_pins": {
        "direct": [
            ["GP27", "GP1", "GP2", "GP3", "GP4", "GP5",  null],
            ["GP28", "GP0", "GP6", "GP7", "GP8", "GP9",  null],
            ["GP29", "GP21", "GP15", "GP14", "GP13", "GP12",  "GP10"],
            ["GP26",  "GP24",   null, "GP16", "GP17", "GP18",  "GP11"],
            ["GP25",  null,   null,   "GP23",  "GP22",  "GP20",  "GP19"]
        ]
    },
	
    "usb": {
        "device_version": "0.6.1",
        "pid": "0x0012",
        "vid": "0x0121",
		"polling_interval":1
    },
	
	"bootmagic": {
		"matrix": [4,2]
	},
	
